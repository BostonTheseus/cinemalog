<% @movies.each do |movie| %>
    <div class="booyah-box col-10 offset-1">
        <h1><%= movie.title %></h1>
        <i><<%= movie.rating %></i><br /><br />
        <p><%= movie.review %></p><br />
        
    <% if place.user.present? %>
      <small><%= place.user.email %></small>
    <% end %>
    
    </div>
    
    <%= will_paginate @movies %>
    
<% end %>